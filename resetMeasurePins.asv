function resetMeasurePins(app,resetButtonState)
% Turn all measurement pins off or on depending on the state of the reset
% button
    stateOfPin = resetButtonState;
    if stateOfPin == 'on
       for i = 1:8 
           togglePinState(app,i,stateOfPin);
       end
    else 
        for i = 1:8 
           togglePinState(app,i,stateOfPin);
        end
    end
end